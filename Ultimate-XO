<audio id="clickSound"><source src="data:audio/wav;base64,UklGRiYAAABXQVZFZm10IBAAAAABAAEAQB8AAAB9AAACABAAZGF0YQIAAAAA" type="audio/wav"></audio>
<audio id="winSound"><source src="data:audio/wav;base64,UklGRiYAAABXQVZFZm10IBAAAAABAAEAQB8AAAB9AAACABAAZGF0YQIAAAAA" type="audio/wav"></audio>

<script>
const bigEl = document.getElementById('bigBoard');
const currentEl = document.getElementById('current');
const allowedEl = document.getElementById('allowed');
const modeEl = document.getElementById('mode');
const messageEl = document.getElementById('message');
const scoreX = document.getElementById('scoreX');
const scoreO = document.getElementById('scoreO');
const scoreD = document.getElementById('scoreD');
const undoBtn = document.getElementById('undo');
const restartBtn = document.getElementById('restart');
const resetScoresBtn = document.getElementById('resetScores');
const toggleAIBtn = document.getElementById('toggleAI');
const soundToggleBtn = document.getElementById('soundToggle');
const clickSound = document.getElementById('clickSound');
const winSound = document.getElementById('winSound');
const difficultySelector = document.getElementById('difficultySelector');

let soundOn = true;
let isAIMode = false;
let aiDifficulty = 2; // 1=easy, 2=medium, 3=hard
let current = 'X';
let boards = Array.from({length:9}, ()=>Array(9).fill(''));
let big = Array(9).fill(null);
let allowed = null;
let history = [];

const LS = 'ultimate_xo_scores_v2';
const saved = JSON.parse(localStorage.getItem(LS) || '{}');
let scores = {X: saved.X||0, O: saved.O||0, D: saved.D||0};
renderScores();

function renderScores(){
  scoreX.textContent = scores.X;
  scoreO.textContent = scores.O;
  scoreD.textContent = scores.D;
  localStorage.setItem(LS, JSON.stringify(scores));
}

function init(){
  bigEl.innerHTML = '';
  for(let b = 0; b < 9; b++){
    const small = document.createElement('div');
    small.className = 'small';
    small.dataset.board = b;
    for(let c = 0; c < 9; c++){
      const cell = document.createElement('div');
      cell.className = 'cell';
      cell.dataset.board = b;
      cell.dataset.cell = c;
      cell.onclick = () => onCellClick(b, c);
      small.appendChild(cell);
    }
    bigEl.appendChild(small);
  }
  updateUI();
}

function onCellClick(b, c){
  if((allowed !== null && allowed !== b) || big[b] || boards[b][c]) return;
  playMove(b, c, current, true);
}

function playMove(b, c, player, record = true){
  if(record) history.push({
    boards: boards.map(x => x.slice()),
    big: big.slice(),
    allowed,
    current
  });
  
  boards[b][c] = player;
  
  if(soundOn) {
    try{ clickSound.currentTime = 0; clickSound.play(); }catch(e){}
  }

  const sw = checkWin(boards[b]);
  if(sw){ 
    big[b] = sw; 
    markBoardWon(b, sw); 
  }

  const bw = checkWin(big);
  if(bw){
    finishGame(bw);
    return;
  }

  // Check for draw
  let allFilled = true;
  for(let i = 0; i < 9; i++){
    if(!big[i]){
      const boardFilled = boards[i].every(cell => cell !== '');
      if(!boardFilled){ allFilled = false; break; }
    }
  }
  if(allFilled){
    finishGame('D');
    return;
  }

  allowed = big[c] ? null : c;
  if(allowed === null) allowedEl.textContent = 'ÙƒÙ„ Ø§Ù„Ù„ÙˆØ­Ø§Øª';
  else allowedEl.textContent = (allowed + 1);

  current = current === 'X' ? 'O' : 'X';
  currentEl.textContent = current;
  updateUI();

  if(isAIMode && current === 'O') setTimeout(aiPlay, 400);
}

function aiPlay(){
  let targetBoards = allowed === null ? getAvailableBoards() : [allowed];
  let chosen = null;
  let chosenMove = null;

  if(aiDifficulty >= 3){
    // Hard: Try to win
    for(const b of targetBoards){
      let winMove = findWinningMove(boards[b], 'O');
      if(winMove !== null){ chosen = b; chosenMove = winMove; break; }
    }
    // Hard: Block player's win
    if(chosen === null){
      for(const b of targetBoards){
        let block = findWinningMove(boards[b], 'X');
        if(block !== null){ chosen = b; chosenMove = block; break; }
      }
    }
  }

  if(chosen === null && aiDifficulty >= 2){
    // Medium: Center
    for(const b of targetBoards){
      if(boards[b][4] === ''){ chosen = b; chosenMove = 4; break; }
    }
  }

  if(chosen === null){
    // Easy/Medium: Corners
    for(const b of targetBoards){
      const corners = [0,2,6,8].filter(i => boards[b][i] === '');
      if(corners.length){ chosen = b; chosenMove = corners[Math.floor(Math.random() * corners.length)]; break; }
    }
  }

  if(chosen === null){
    // Random
    for(const b of targetBoards){
      const free = boards[b].map((v,i) => v === '' ? i : null).filter(v => v !== null);
      if(free.length){ chosen = b; chosenMove = free[Math.floor(Math.random() * free.length)]; break; }
    }
  }

  if(chosen === null){
    const ab = getAvailableBoards();
    if(ab.length){
      chosen = ab[Math.floor(Math.random() * ab.length)];
      const free = boards[chosen].map((v,i) => v === '' ? i : null).filter(v => v !== null);
      chosenMove = free[Math.floor(Math.random() * free.length)];
    }
  }

  if(chosen !== null && chosenMove !== null) playMove(chosen, chosenMove, 'O', true);
}

function getAvailableBoards(){
  const arr = [];
  for(let i = 0; i < 9; i++){
    if(!big[i]){
      const isFilled = boards[i].every(cell => cell !== '');
      if(!isFilled) arr.push(i);
    }
  }
  return arr;
}

function findWinningMove(bArr, player){
  for(let i = 0; i < 9; i++){
    if(bArr[i] === ''){
      const copy = bArr.slice();
      copy[i] = player;
      if(checkWin(copy) === player) return i;
    }
  }
  return null;
}

function checkWin(arr){
  const lines = [[0,1,2],[3,4,5],[6,7,8],[0,3,6],[1,4,7],[2,5,8],[0,4,8],[2,4,6]];
  for(const [a,b,c] of lines){
    if(arr[a] && arr[a] === arr[b] && arr[a] === arr[c]) return arr[a];
  }
  return null;
}

function markBoardWon(b, w){
  const boardEl = document.querySelectorAll('.small')[b];
  boardEl.classList.add('won');
  boardEl.setAttribute('data-winner', w);
  boardEl.querySelectorAll('.cell').forEach(e => {
    e.classList.add('disabled');
    e.textContent = '';
    e.classList.remove('x', 'o');
  });
}

function finishGame(winner){
  if(winner === 'D'){
    messageEl.textContent = 'ğŸ¤ Ø§Ù†ØªÙ‡Øª Ø§Ù„Ù…Ø¨Ø§Ø±Ø§Ø©: ØªØ¹Ø§Ø¯Ù„';
    scores.D++;
  } else {
    messageEl.textContent = `ğŸ‰ Ø§Ù„ÙØ§Ø¦Ø²: ${winner}`;
    scores[winner]++;
    if(soundOn){
      try{ winSound.currentTime = 0; winSound.play(); }catch(e){}
    }
  }
  renderScores();
  document.querySelectorAll('.cell').forEach(e => e.classList.add('disabled'));
}

function updateUI(){
  document.querySelectorAll('.cell').forEach(el => {
    const b = +el.dataset.board;
    const c = +el.dataset.cell;
    el.textContent = boards[b][c];
    el.classList.remove('x', 'o');
    if(boards[b][c] === 'X') el.classList.add('x');
    if(boards[b][c] === 'O') el.classList.add('o');
    if(big[b] || boards[b][c] || (allowed !== null && allowed !== b)) el.classList.add('disabled');
    else el.classList.remove('disabled');
  });

  document.querySelectorAll('.small').forEach((sm, i) => {
    sm.classList.remove('allowed', 'won');
    if(big[i]) sm.classList.add('won');
    else if(allowed === null || allowed === i) sm.classList.add('allowed');
  });

  modeEl.textContent = isAIMode ? 'Ù„Ø§Ø¹Ø¨ Ã— Ø­Ø§Ø³ÙˆØ¨' : 'Ù„Ø§Ø¹Ø¨ Ã— Ù„Ø§Ø¹Ø¨';
  if(allowed === null) allowedEl.textContent = 'ÙƒÙ„ Ø§Ù„Ù„ÙˆØ­Ø§Øª';
  else allowedEl.textContent = (allowed + 1);
}

undoBtn.addEventListener('click', () => {
  if(!history.length) return;
  const last = history.pop();
  boards = last.boards.map(x => x.slice());
  big = last.big.slice();
  allowed = last.allowed;
  current = last.current;
  currentEl.textContent = current;
  messageEl.textContent = 'â†¶ ØªÙ… Ø§Ù„ØªØ±Ø§Ø¬Ø¹';
  updateUI();
});

restartBtn.addEventListener('click', () => {
  boards = Array.from({length:9}, () => Array(9).fill(''));
  big = Array(9).fill(null);
  allowed = null;
  current = 'X';
  history = [];
  messageEl.textContent = 'ğŸ‘‹ Ù„Ø¹Ø¨Ø© Ø¬Ø¯ÙŠØ¯Ø© - ÙŠÙ„Ø¹Ø¨ X Ø£ÙˆÙ„Ø§Ù‹';
  currentEl.textContent = current;
  updateUI();
});

resetScoresBtn.addEventListener('click', () => {
  scores = {X:0, O:0, D:0};
  renderScores();
  messageEl.textContent = 'ğŸ”„ ØªÙ… Ù…Ø³Ø­ Ø§Ù„Ù†Ù‚Ø§Ø·';
});

toggleAIBtn.addEventListener('click', () => {
  isAIMode = !isAIMode;
  toggleAIBtn.textContent = isAIMode ? 'ğŸ¤– AI: ØªØ´ØºÙŠÙ„' : 'ğŸ¤– AI: Ø¥ÙŠÙ‚Ø§Ù';
  difficultySelector.style.display = isAIMode ? 'flex' : 'none';
  if(isAIMode && current === 'O') setTimeout(aiPlay, 250);
  messageEl.textContent = isAIMode ? 'ğŸ¤– ØªÙ… ØªØ´ØºÙŠÙ„ AI' : 'ğŸ‘¤ ÙˆØ¶Ø¹ Ø§Ù„Ù„Ø§Ø¹Ø¨';
  updateUI();
});

soundToggleBtn.addEventListener('click', () => {
  soundOn = !soundOn;
  soundToggleBtn.textContent = soundOn ? 'ğŸ”Š ØµÙˆØª: ØªØ´ØºÙŠÙ„' : 'ğŸ”‡ ØµÙˆØª: Ø¥ÙŠÙ‚Ø§Ù';
  messageEl.textContent = soundOn ? 'ğŸ”Š Ø§Ù„Ø£ØµÙˆØ§Øª Ù…Ø´ØºÙ„Ø©' : 'ğŸ”‡ Ø§Ù„Ø£ØµÙˆØ§Øª Ù…Ø¹Ø·Ù„Ø©';
});

document.querySelectorAll('.difficulty-btn').forEach(btn => {
  btn.addEventListener('click', (e) => {
    document.querySelectorAll('.difficulty-btn').forEach(b => b.classList.remove('active'));
    e.target.classList.add('active');
    aiDifficulty = +e.target.dataset.level;
    const levels = ['Ø³Ù‡Ù„', 'Ù…ØªÙˆØ³Ø·', 'ØµØ¹Ø¨'];
    messageEl.textContent = `âš™ï¸ Ø§Ù„ØµØ¹ÙˆØ¨Ø©: ${levels[aiDifficulty - 1]}`;
  });
});

